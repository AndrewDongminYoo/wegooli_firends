{
  "[dart]": {
    "cSpell.fixSpellingWithRenameProvider": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.indentSize": "tabSize",
    "editor.insertSpaces": true,
    "editor.lightbulb.enabled": true,
    "editor.selectionHighlight": false,
    "editor.suggest.preview": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 2,
    "editor.wordBasedSuggestions": true,
    "editor.wordBasedSuggestionsMode": "allDocuments"
  },
  "cSpell.ignorePaths": [
    ".git/objects",
    ".gitignore",
    ".vscode",
    "lib/theme/color_names.dart",
    "package.json",
    "*.plist",
    "*.xcconfig",
    "*.xcassets",
    "*.appiconset",
    "*.imageset",
    "*.lproj",
    "*.xcodeproj",
    "*.xcworkspace",
    "*.xcsettings",
    "*.xcworkspacedata",
    "*.lock"
  ],
  "cSpell.words": [
    "ADID",
    "ARGB",
    "Bodymovin",
    "bools",
    "CMYKA",
    "daum",
    "FFRRGGBB",
    "Getx",
    "GLLO",
    "gooli",
    "HWBA",
    "IDFA",
    "interps",
    "kapi",
    "Kpostal",
    "Luhn",
    "NCOL",
    "Noto",
    "regexs",
    "Roboto",
    "signin",
    "signup",
    "sqflite",
    "Symbolication",
    "Todos",
    "Totp",
    "unfocus",
    "vsync",
    "wegooli",
    "xclip",
    "xcworkspace",
    "xxxkm",
    "Zeplin"
  ],
  "css.lint.unknownProperties": "ignore",
  "dart.buildRunnerAdditionalArgs": ["--delete-conflicting-outputs"],
  "dart.documentation": "full",
  "dart.enableSdkFormatter": true,
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift",
  "dart.flutterCreatePlatforms": ["android", "ios", "macos", "web"],
  "dart.flutterHotReloadOnSave": "all",
  "dart.flutterWebRenderer": "auto",
  "dart.openDevTools": "always",
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "dart.updateImportsOnRename": true,
  "dartBarrelFileGenerator.excludeDirList": ["gen", "api", "bloc"],
  "dartBarrelFileGenerator.excludeFreezed": true,
  "dartBarrelFileGenerator.excludeGenerated": true,
  "dartBarrelFileGenerator.skipEmpty": true,
  "docwriter.language": "Korean",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.dart": "${capture}.*.dart"
  },
  "files.associations": {
    "*.arb": "json",
    ".openapi-generator-ignore": "ignore"
  },
  "jsonToDart.avoidDynamicTypes": false,
  "jsonToDart.codeGenerator": "JSON",
  "jsonToDart.copyWith": true,
  "jsonToDart.equality": "Equatable",
  "jsonToDart.fastMode": true,
  "jsonToDart.immutable": true,
  "jsonToDart.json.codecs": true,
  "jsonToDart.nullSafety": true,
  "jsonToDart.primaryConfiguration": true,
  "jsonToDart.runBuilder": false,
  "jsonToDart.sortConstructorsFirst": true,
  "jsonToDart.targetDirectory.path": "/lib/data/",
  "jsonToDart.toString": "Auto"
}
