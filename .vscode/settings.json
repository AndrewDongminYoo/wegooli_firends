{
  "cSpell.ignorePaths": [
    ".git/objects",
    ".vscode",
    "lib/theme/color_names.dart",
    "package.json",
    "*.plist",
    "*.xcconfig",
    "*.xcassets",
    "*.appiconset",
    "*.imageset",
    "*.lproj",
    "*.xcodeproj",
    "*.xcworkspace",
    "*.xcsettings",
    "*.xcworkspacedata",
    "*.lock"
  ],
  "cSpell.words": [
    "ARGB",
    "Bodymovin",
    "bools",
    "CMYKA",
    "daum",
    "FFRRGGBB",
    "Getx",
    "GLLO",
    "gooli",
    "HWBA",
    "interps",
    "kapi",
    "Kpostal",
    "NCOL",
    "Noto",
    "Roboto",
    "signin",
    "signup",
    "sqflite",
    "Symbolication",
    "Todos",
    "Totp",
    "unfocus",
    "vsync",
    "wegooli",
    "xclip",
    "xcworkspace",
    "xxxkm",
    "Zeplin"
  ],
  "dart.documentation": "full",
  "dart.enableSdkFormatter": true,
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift",
  "dart.flutterCreatePlatforms": ["android", "ios", "macos", "web"],
  "dart.flutterHotReloadOnSave": "all",
  "dart.flutterWebRenderer": "auto",
  "dart.openDevTools": "flutter",
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.showInspectorNotificationsForWidgetErrors": false,
  "dart.updateImportsOnRename": true,
  "dartBarrelFileGenerator.excludeDirList": [
    "gen",
    "generated",
    "binding",
    "controller",
    "api"
  ],
  "dartBarrelFileGenerator.excludeFreezed": true,
  "dartBarrelFileGenerator.excludeGenerated": true,
  "dartBarrelFileGenerator.skipEmpty": true,
  "docwriter.language": "Korean",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.dart": "${capture}.*.dart"
  },
  "files.associations": {
    "*.arb": "json",
    ".openapi-generator-ignore": "ignore"
  },
  "jsonToDart.avoidDynamicTypes": false,
  "jsonToDart.codeGenerator": "JSON",
  "jsonToDart.copyWith": true,
  "jsonToDart.equality": "Equatable",
  "jsonToDart.fastMode": true,
  "jsonToDart.immutable": true,
  "jsonToDart.json.codecs": true,
  "jsonToDart.nullSafety": true,
  "jsonToDart.primaryConfiguration": true,
  "jsonToDart.runBuilder": false,
  "jsonToDart.sortConstructorsFirst": true,
  "jsonToDart.targetDirectory.path": "/lib/data/",
  "jsonToDart.toString": "Auto"
}
