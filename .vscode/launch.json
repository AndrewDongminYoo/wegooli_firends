    "version": "0.2.0",
    "configurations": [
        {
            "name": "wegooli_friends",
            "request": "launch",
            "type": "dart",
            "flutterPlatform": "default",
            "flutterMode": "debug",
            "console": "debugConsole",
            "args": [
                "--target=lib/main.dart",
                "--hot",
                "--ignore-deprecation",
                "--route=/shared_schedule",
                "--verbose"
            ],
            "openDevTools": "logging",
            "program": "lib/main.dart",
            "suppressWebServerDeviceBrowserLaunch": true
        },
        {
            "name": "wegooli_friends (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "wegooli_friends (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}